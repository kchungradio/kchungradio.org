{
  "routes": [
    {
      "src": "/",
      "status": 307,
      "headers": {
        "Location": "/stream"
      }
    },
    {
      "src": "/api(.*)",
      "methods": [
        "GET"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "s-maxage=300, max-age=0"
      },
      "continue": true
    },
    {
      "src": "/([^/]+).html",
      "status": 301,
      "headers": {
        "Location": "/$1"
      }
    },
    {
      "src": "/wiki(.*)",
      "status": 301,
      "headers": {
        "Location": "https://wiki.kchungradio.org$1"
      }
    },
    {
      "src": "/Talking-Show(.*)",
      "status": 301,
      "headers": {
        "Location": "https://talking-show.kchungradio.org$1"
      }
    },
    {
      "src": "/([^/]+).(jpg|png|ico|svg)",
      "status": 301,
      "headers": {
        "Location": "/img/$1.$2"
      }
    }
  ]
}
